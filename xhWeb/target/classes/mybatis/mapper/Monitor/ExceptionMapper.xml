<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >
<mapper namespace="com.cssnb.monitor.dao.ExceptionDao">
     <!-- 将捕获的操作存入数据库 -->
     <insert id="insertException" parameterType="java.util.Map">
	insert into XT_EXCEPTION_LOG(YCDM,YCXX,YCFF,FFMS,QQR,IP,QQCS,URL,YCL)
	values(#{YCDM},#{YCXX},#{YCFF},#{FFMS},#{QQR},#{IP},#{QQCS},#{URL},#{YCL})
     </insert>
</mapper>