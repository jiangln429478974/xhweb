<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >
<mapper namespace="com.cssnb.monitor.dao.MonitorDao" >
     <!-- 将捕获的操作存入数据库 -->
     <insert id="insertMonitor" parameterType="java.util.Map">
	insert into GG_MONITOR(USERNAME,OPERATINGDATE,CLASSNAME,METHODNAME,IP,URL)
	values(#{USERNAME},#{OPERATINGDATE},#{CLASSNAME},#{METHODNAME},#{IP},#{URL},#{CS})
     </insert>
</mapper>