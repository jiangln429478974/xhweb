<!--     定时更新联合激励发布状态 -->
    <bean id="LhjlController" class="com.cssnb.lhcj.controller.LhjlController"></bean>
    
<bean id="gsDetail" class="org.springframework.scheduling.quartz.MethodInvokingJobDetailFactoryBean">
  	 <property name="targetObject"><ref bean="ToSendMSG" /></property>
  	 <property name="targetMethod" value="toSend" />
  	 <property name="concurrent" value="false" ></property>
</bean>
<bean id="yyclDetail" class="org.springframework.scheduling.quartz.MethodInvokingJobDetailFactoryBean">
  	 <property name="targetObject"><ref bean="yycl" /></property>
  	 <property name="targetMethod" value="dstx" />
  	 <property name="concurrent" value="false" ></property>
</bean>
<!-- part2  -->
<!--联合惩戒文件异常发送短信以及站内消息   -->
