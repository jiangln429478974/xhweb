    <!-- 信用信息清洗比对情况报表 -->
    <bean id="ToGetXYGJ2" class="com.cssnb.pttj.controller.MzzbController"></bean>
    <!-- 自然人公共信用信息归集情况 -->
    <bean id="ToGetSJGJ" class="com.cssnb.pttj.controller.MzzbController"></bean>
     <!-- 自然人公共信用信息归集情况0614 -->
    <bean id="ToGetSJGJ0614" class="com.cssnb.pttj.controller.MzzbController"></bean>
    <!-- 总体情况 -->
    <bean id="ToGetZTQK" class="com.cssnb.pttj.controller.MzzbController"></bean>
    <!-- 更新文件报送接口JXZT -->
