<bean id="fgsJob"
	class="org.springframework.scheduling.quartz.CronTriggerFactoryBean">
	<!-- Seconds Minutes Hours Day-of-Month Month Day-of-Week -->
	<property name="cronExpression">
		<value>0 1/1 9-17 ? * MON-FRI </value>
	</property>
	<property name="jobDetail">
		<ref bean="gsDetail" />
	</property>
</bean>
<bean id="yyclJob"
class="org.springframework.scheduling.quartz.CronTriggerFactoryBean">
	<!-- Seconds Minutes Hours Day-of-Month Month Day-of-Week -->
	<property name="cronExpression">
	<value>0 30 21 ? * MON-FRI</value>
	</property>
	<property name="jobDetail">
		<ref bean="yyclDetail" />
</property>
</bean>
<!-- part3 -->
<bean id="lhcjJob" class="org.springframework.scheduling.quartz.CronTriggerFactoryBean">
	<property name="cronExpression">
	<!-- 每隔五分钟 -->
		   <value>0 0/5 * * * ?</value>  
	</property>
	<property name="jobDetail">
		<ref bean="lhcjmsgDetail" />
	</property>
</bean>
