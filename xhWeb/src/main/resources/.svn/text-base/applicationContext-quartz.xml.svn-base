     <property name="concurrent" value="false"></property>
</bean>

<!-- 总体情况  -->
<bean id="ztqkDetail" class="org.springframework.scheduling.quartz.MethodInvokingJobDetailFactoryBean">
     <property name="targetObject"><ref bean="ToGetZTQK"/></property>
     <property name="targetMethod" value="Ztqk"/>
     <property name="concurrent" value="false"></property>
</bean>
<!-- 更新文件报送接口JXZT -->
